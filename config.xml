<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tracking.nextrack" version="0.1">
    <name>Nextrack</name>
    <description>
        Aplicaci√≥n de rastreo satelital. Propiedad de Nexing S.A.S.
    </description>
    <author email="jdjd24@gmail.com" href="http://nexing.com.co">
        Jaime Bermeo (Penk)
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <custom-preference name="android-manifest/application/@android:hardwareAccelerated" value="true" />
        <custom-preference name="android-manifest/@android:hardwareAccelerated" value="true" />
        <allow-intent href="market:*" />
        <preference name="android-minimumSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="25" />
        <preference name="loadUrlTimeoutValue" value="700000" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-secure-storage" spec="^2.6.8" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-background-mode" spec="https://github.com/katzer/cordova-plugin-background-mode.git" />
    <engine name="android" spec="^7.0.0" />
</widget>
